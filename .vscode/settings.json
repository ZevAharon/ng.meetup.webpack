{
    "editor.formatOnSave": false,
    "search.exclude": {
        ".nuget": true,
        ".vs": true,
        ".vscode": true,
        "*.csproj*": true,
        "*.sln*": true,
        "**/bower_components": true,
        "**/node_modules": false,
        "*packages.config*": true,
        "$tf": true,
        "App_Data": true,
        "App_Start": true,
        "Attributes": true,
        "BackgroundProcess": true,
        "bin": true,
        "CachedObject.cs": true,
        "CacheProvider.cs": true,
        "Classes": true,
        "CodeGen": true,
        "Components": true,
        "Configuration": true,
        "Constants": true,
        "Contracts": true,
        "Controllers": true,
        "DependencyResolution": true,
        "dist": true,
        "Errors": true,
        "Extensions": true,
        "ExternalBinaries": true,
        "Filters": true,
        "Global.asax*": true,
        "Handlers": true,
        "Help": true,
        "Helpers": true,
        "Hubs": true,
        "License": true,
        "Listeners": true,
        "Models": true,
        "Modules": true,
        "obj": true,
        "packages": true,
        "Properties": true,
        "Services": true,
        "user.config": true,
        "Viewers": true,
        "webdeploy": true
    },
    "beautify.config": {
        "wrap_attributes": "force-aligned",
        "indent_with_tabs": false,
        "max_preserve_newlines": 2,
        "preserve_newlines": true,
        "keep_array_indentation": true,
        "break_chained_methods": true,
        "wrap_line_length": 140,
        "end_with_newline": true,
        "brace_style": "collapse,preserve-inline"
    },
    "auto-close-tag.activationOnLanguage": [
        "html",
        "xml",
        "javascript",
        "javascriptreact"
    ]
}
